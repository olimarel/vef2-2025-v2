<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Quiz Categories</title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <h1>Quiz Categories</h1>
    <ul>
      <% categories.forEach(category => { %>
        <li><a href="/questions/<%= category.id %>"><%= category.name %></a></li>
      <% }) %>
    </ul>
  </body>
</html>
