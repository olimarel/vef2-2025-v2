<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Error</title>
    <link rel="stylesheet" href="/styles.css">
  </head>
  <body>
    <div class="container">
      <h1>Something Went Wrong</h1>
      <p><%= message %></p>
      <% if (error && error.stack) { %>
        <pre><%= error.stack %></pre>
      <% } %>
      <a href="/">Return to Home</a>
    </div>
  </body>
</html>
